{
  "name": "ascii2mathml",
  "version": "0.5.3",
  "description": "The second AsciiMath – MathML converter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runarberg/ascii2mathml.git"
  },
  "main": "index.js",
  "bin": {
    "ascii2mathml": "bin/index.js"
  },
  "scripts": {
    "babelify": "babel src/ -d lib/; babel < index.es6.js > index.js",
    "browserify": "(echo \"/*! ${npm_package_name} v${npm_package_version} | (c) 2015 (MIT) | ${npm_package_homepage} */\"; browserify ./index.es6.js -s ascii2mathml) > dist/ascii2mathml.js",
    "minify": "uglifyjs dist/ascii2mathml.js -b beautify=false,ascii-only=true -c -m --preamble \"/*! ${npm_package_name} v${npm_package_version} | (c) 2015 (MIT) | ${npm_package_homepage} */\" > dist/ascii2mathml.min.js",
    "lint": "eslint index.es6.js src/",
    "prebuild": "npm run lint",
    "build": "rm -fr lib; mkdir lib; npm run babelify",
    "predist": "npm run lint",
    "dist": "rm -fr dist; mkdir dist; npm run browserify; npm run minify",
    "prepublish": "npm run build; npm run dist",
    "postpublish": "tmp=$(mktemp -d ${TMPDIR}/asci2mathml.XXXXX) && cp -r dist $tmp/ && git checkout gh-pages && cp ${tmp}/dist/* dist/ && git commit -am \"v${npm_package_version}-public\" && git checkout dist && cp ${tmp}/dist/* dist/ && git commit -am \"v${npm_package_version}-dist\"; git checkout master",
    "pretest": "npm run build",
    "test": "mocha"
  },
  "keywords": [
    "mathml",
    "asciimath",
    "asciimathml"
  ],
  "author": {
    "name": "Rúnar Berg Baugsson Sigríðarson"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "eslint": "^1.0.0",
    "eslint-plugin-nodeca": "^1.0.3",
    "expect.js": "^0.3.1",
    "mocha": "^2.1.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "babel": "^5.8.19",
    "minimist": "^1.1.2"
  },
  "gitHead": "02aec43424b2dbe7854da45a0a9bdac227a9a395",
  "bugs": {
    "url": "https://github.com/runarberg/ascii2mathml/issues"
  },
  "homepage": "https://github.com/runarberg/ascii2mathml#readme",
